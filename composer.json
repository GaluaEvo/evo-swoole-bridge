{
    "name": "galuaevo/evo-swoole-bridge",
    "description": "Swoole Bridge For Php Frameworks",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "fuong",
            "email": "insidestyles@gmail.com"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": "^7.1|^8.0",
        "ext-swoole": ">=2.1.0",
        "psr/log": "^1.0|^2.0|^3.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "symfony/psr-http-message-bridge": "^2.0",
        "laminas/laminas-diactoros": "^2.0",
        "nyholm/psr7": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^9.0",
        "eaglewu/swoole-ide-helper": "dev-master",
        "symfony/http-kernel": "^4.0|^5.0|^6.0",
        "zendframework/zend-diactoros": "^1.7|^2.0",
        "illuminate/contracts": "^5.6|^6.0|^7.0|^8.0"
    },
    "suggest": {
        "symfony/psr-http-message-bridge": "Allows builder for Symfony http message",
        "symfony/http-kernel": "Allows adapter for symfony",
        "zendframework/zend-expressive": "Allows adapter for Zend Expressive"
    },
    "autoload": {
        "psr-4": {
            "Insidestyles\\SwooleBridge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Insidestyles\\SwooleBridge\\Tests\\": "tests/"
        }
    }
}
